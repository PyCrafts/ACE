server {
    listen 443 ssl default_server;
    server_name ace-http;
    ssl_certificate /opt/ace/ssl/ace.cert.pem;
    ssl_certificate_key /opt/ace/ssl/ace.key.pem;

    location / {
        include uwsgi_params;
        uwsgi_pass ace:3031;
    }

    location /api {
        include uwsgi_params;
        uwsgi_pass ace:3032;
    }
}
